{
  "name": "ytFastForward",
  "description": "Enables you to fast forward beyond 2x on youtube.",
  "version": "1.0",
  "permissions": [
    "tabs", "http://youtube.com/*", "https://youtube.com/*", "http://www.youtube.com/*", "https://www.youtube.com/*"
  ],
  "browser_action": {
      "default_title": "Set speed",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },

  "manifest_version": 2
}
